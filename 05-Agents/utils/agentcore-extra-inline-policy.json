{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowBedrockRetrieve",
      "Effect": "Allow",
      "Action": [
        "bedrock:Retrieve"
      ],
      "Resource": "*"
    },
    {
      "Sid": "AllowDynamoDBCrud",
      "Effect": "Allow",
      "Action": [
        "dynamodb:GetItem",
        "dynamodb:PutItem",
        "dynamodb:DeleteItem"
      ],
      "Resource": "*"
    }
  ]
}
